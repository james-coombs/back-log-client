<div class="col-sm-12 back-logs-table">
  <div>
    {{#each logs}}
    <div class="row">
      <div class="col-sm-4">
        <h6>{{id}}</h6>
      </div>
      <div class="col-sm-4">
        <h6>{{title}}</h6>
      </div>
      <div class="col-sm-2">
        <button class="delete-log" id="{{id}}" data-control="deleteLog">Delete</button>
      </div>
      <div class="col-sm-2">
        <button class="show-log" id="{{id}}" data-control="showLog">Show</button>
      </div>
      </div>
    {{/each}}
  </div>
</div>
