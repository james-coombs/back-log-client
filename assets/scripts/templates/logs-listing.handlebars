<div class="back-logs-table">
  <h1>My Backlog:</h1>
  <table>
    <colgroup span="6"></colgroup>
    <thead>
      <th>Log Id</th>
      <th>Title</th>
      <th>Platform</th>
      <th>Players</th>
      <th>Stage</th>
      <th>Notes</th>
    </thead>
    <tbody>
      {{#each logs}}
        <tr>
          <td>{{id}}</td>
          <td>{{title}}</td>
          <td>{{platform}}</td>
          <td>{{players}}</td>
          <td>{{stage}}</td>
          <td>{{notes}}</td>
        </tr>
      {{/each}}
    </tbody>
  </table>
</div>
