

<h1>My Backlog:</h1>
<table>
  <colgroup span="6"></colgroup>
  </colgroup>
    <thead class="log-thead">
        <th>Id</th>
        <th>Title</th>
        <th>Platform</th>
        <th>Players</th>
        <th>Stage</th>
        <th>Notes</th>
    </thead>
    <tbody>
        {{#each logs}}
        <tr>
            <td>{{id}}</td>
            <td>{{title}}</td>
            <td>{{platform}}</td>
            <td>{{players}}</td>
            <td>{{stage}}</td>
            <td>{{notes}}</td>
        </tr>
        {{/each}}
    </tbody>
</table>
